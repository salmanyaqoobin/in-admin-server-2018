{
  "name": "INadmin",
  "version": "1.0.0",
  "description": "IN Admin Control application for building REST APIs using express, mongoose in ES6 with code coverage",
  "author": "Salman Yaqoob <sy@in-hq.com>",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ngstart": "ng serve",
    "ngbuild": "ng build --prod",
    "ngtest": "ng test",
    "nglint": "ng lint",
    "e2e": "ng e2e",
    "start": "node index.js",
    "start:debug": "cross-env DEBUG=express-admin:* yarn start",
    "build": "gulp",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/salmanyaqoobin/in-admin-server-2018.git"
  },
  "dependencies": {
    "async": "^2.6.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "3.5.0",
    "body-parser": "1.17.2",
    "compression": "1.7.0",
    "connect-multiparty": "^2.1.0",
    "cookie-parser": "1.4.3",
    "core-js": "^2.4.1",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "expect": "^22.0.3",
    "express": "4.15.4",
    "express-jwt": "5.3.0",
    "express-validation": "1.0.2",
    "express-winston": "2.4.0",
    "helmet": "3.8.1",
    "http-status": "1.0.1",
    "joi": "10.6.0",
    "jsonwebtoken": "7.1.9",
    "lodash": "^4.17.4",
    "marked": "^0.3.9",
    "method-override": "^2.3.5",
    "mongodb": "3.0.1",
    "mongoose": "4.7.4",
    "morgan": "1.8.2",
    "multer": "1.3.0",
    "rxjs": "^5.5.2",
    "validator": "^9.2.0",
    "winston": "2.3.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "^6.26.0",
    "@babel/register": "7.0.0-beta.39",
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "husky": "^0.13.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.2.0",
    "run-sequence": "^1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  }
}
